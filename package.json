{
	"name": "klara",
	"packageManager": "yarn@3.0.2",
	"scripts": {
		"dev": "clean && concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
		"watch-compile": "swc src/ -w --out-dir out",
		"watch-dev": "nodemon --watch \"out/**/*\" -e js ./out/index.js",
		"build": "clean && swc src -d out",
		"start": "NODE_ENV=production node out/index.js",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"discord.js": "^13.6.0",
		"dotenv": "^16.0.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.174",
		"@yarnpkg/sdks": "^3.0.0-rc.4",
		"chokidar": "^3.5.3",
		"concurrently": "^7.1.0",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"nodemon": "^2.0.16",
		"prettier": "^2.6.2"
	},
	"dependenciesMeta": {
		"discord.js@13.6.0": {
			"unplugged": true
		}
	}
}
