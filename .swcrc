{
	"module": {
		"type": "commonjs"
	},
	"jsc": {
		"parser": {
			"syntax": "ecmascript",
			"jsx": false,
			"dynamicImport": true,
			"privateMethod": false,
			"functionBind": false,
			"exportDefaultFrom": false,
			"exportNamespaceFrom": false,
			"decorators": false,
			"decoratorsBeforeExport": false,
			"topLevelAwait": true,
			"importMeta": false
		},
		"transform": null,
		"target": "es2021",
		"loose": false,
		"externalHelpers": false,
		// Requires v1.2.50 or upper and requires target to be es2016 or upper.
		"keepClassNames": true
	}
}
